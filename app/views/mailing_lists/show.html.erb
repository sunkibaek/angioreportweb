<div class="container">
  <h1>Mailing list subscribers</h1>

  <table class="table">
    <tbody>
      <% @mailing_lists.each do |mailing_list| %>
        <tr>
          <td><%= mailing_list.email %></td>
          <td><%= mailing_list.created_at %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>